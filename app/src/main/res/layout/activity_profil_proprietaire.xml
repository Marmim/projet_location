<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:orientation="vertical"
    android:layout_width="360dp"
    android:layout_height="850dp"
    tools:context=".ProfilProprietaire">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">
    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_chevron_left_24"
        android:text="Propriétaire"
        android:textAlignment="center"
        android:textSize="20dp"/>
    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/femalepro" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Changer la photo de profile"
        android:textAlignment="center"
        android:textColor="@color/blue"
        android:layout_marginBottom="100dp"/>
    <TextView
        android:id="@+id/editTextText5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_person_24"
        android:text="  informations personnelles"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_payment_24"
        android:text="  Paiements et versements"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_notifications_24"
        android:text="  Notifications"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_favorite_24"
        android:text="  Favoris"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_playlist_add_24"
        android:text="  Publier une annonce"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_dashboard_24"
        android:text="  Dashboard"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_format_list_bulleted_24"
        android:text="  Liste de locataire"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    <TextView
        android:id="@+id/editTextText12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:ems="10"
        android:drawableLeft="@drawable/baseline_logout_24"
        android:text="  Se déconnecter"
        android:drawableRight="@drawable/baseline_chevron_right_24"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"/>
    </LinearLayout>
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:menu="@menu/bottom_menu"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="619dp"
        app:itemIconTint="@color/black"
        app:labelVisibilityMode="labeled"/>

</RelativeLayout>